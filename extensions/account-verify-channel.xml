<?xml version="1.0" ?>
<node name="/Channel_Interface_account_verify" xmlns:tp="http://telepathy.freedesktop.org/wiki/DbusSpec#extensions-v0">
  <interface name="org.freedesktop.Telepathy.Channel.Interface.Facebook.AccountVerify">

    <property name="VerifyUrl" type="s" access="read"/>
    <property name="Title" type="s" access="read"/>
    <property name="Message" type="s" access="read"/>

    <method name="AccountVerified" tp:name-for-bindings="Account_Verified">
      <arg name="verified" type="b">
        <tp:docstring>
          Result of account verification on url that was passed with VerifyUrl
        </tp:docstring>
      </arg>
      <tp:docstring>
        Method to be called when account verification is complete.
      </tp:docstring>
    </method>

  </interface>
</node>
